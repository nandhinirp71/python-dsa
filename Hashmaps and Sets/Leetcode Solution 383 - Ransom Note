from collections import Counter
class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        mag_dict = dict(Counter(magazine))

        print(mag_dict)

        for i in ransomNote:
            if i not in mag_dict:
                return False
            elif i in mag_dict and mag_dict[i] > 1:
                mag_dict[i] -= 1  
            elif i in mag_dict:
                del mag_dict[i]
        
        return True
